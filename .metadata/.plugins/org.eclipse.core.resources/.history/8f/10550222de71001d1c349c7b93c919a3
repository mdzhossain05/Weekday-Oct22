package functionLearning;

//runnable class
public class LearingVariable {

	public static void main(String[] args) {
		
		//Global variable
		int a = 10; 
		
		int b = 20;
		
		{
			// Local variable
			int c = 30;
			System.out.println(c);
			System.out.println(a);
		}
		
		int c = 40;
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		
	}

}
