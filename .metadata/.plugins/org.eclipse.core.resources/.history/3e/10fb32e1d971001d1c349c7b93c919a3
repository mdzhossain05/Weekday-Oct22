package functionLearning;

public class AddingIntNumbers {
	
	public void addingTwoIntNumbers(int a, int b) {
		int c = a + b;
		System.out.println(c);
	}
	
	public void addingThreeIntNumbers(int d, int e, int f) {
		int g = d + e + f;
		System.out.println(g);
	}
	
	public void addingFourIntNumbers(int h, int i, int j, int m) {
		int k = h + i + j + m;
		System.out.println(k);
	}
	
	// Create a function, which will take your string name and int zip code
	// and print it in the console. 
		
	public void addingStringAndInt(String name) {
		System.out.println(name);
	}

}
